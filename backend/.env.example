# Backend Environment Variables
# Copy this to .env and fill in your values

# Server port
PORT=5001

# MongoDB connection string
MONGODB_URI=mongodb+srv://username:password@cluster.mongodb.net/quiniela?retryWrites=true&w=majority

# JWT secret for authentication (use a strong random string)
JWT_SECRET=your_super_secret_jwt_key_change_in_production

# API Football key (optional)
API_FOOTBALL_KEY=your_api_football_key

# Frontend URL for CORS (set after deploying frontend to Vercel)
FRONTEND_URL=https://your-app.vercel.app
